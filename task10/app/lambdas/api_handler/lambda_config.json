{
	"version": "1.0",
	"name": "api_handler",
	"func_name": "lambdas/api_handler/index.handler",
	"resource_type": "lambda",
	"iam_role_name": "api_handler-role",
	"runtime": "nodejs20.x",
	"memory": 128,
	"timeout": 100,
	"lambda_path": "lambdas\\api_handler",
	"dependencies": [
		{
			"resource_name": "Tables",
			"resource_type": "dynamodb_table"
		},
		{
			"resource_name": "Reservations",
			"resource_type": "dynamodb_table"
		},
		{
			"resource_name": "simple-booking-userpool",
			"resource_type": "cognito_idp"
		}
	],
	"event_sources": [],
	"env_variables": {
		"CUPId": {
			"resource_name": "simple-booking-userpool",
			"resource_type": "cognito_idp",
			"parameter": "id"
		},
		"CUPClientId": {
			"resource_name": "simple-booking-userpool",
			"resource_type": "cognito_idp",
			"parameter": "client_id"
		}
	},
	"publish_version": true,
	"alias": "${lambdas_alias_name}",
	"url_config": {},
	"ephemeral_storage": 512,
	"tags": {}
}
